{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "publicPath": "public_html/app/themes/{{ cookiecutter.project_slug }}",
  "devUrl": "http://{{ cookiecutter.project_slug.replace('_', '-') }}.lo/",
  "proxyUrl": "http://{{ cookiecutter.project_slug.replace('_', '-') }}.lo:3000/",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/**/*.php"
  ]
}
